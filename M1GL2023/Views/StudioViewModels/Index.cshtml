        <!--</th>-->
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SuperficiBien)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Localisation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Meuble)
